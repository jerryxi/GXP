        //检查用户名是否存在
        function CheckItemInfoIsTrue() {
            $("#UserCode").blur(function () {
                if (typeof ($("#UserCode").attr("readonly")) == "undefined") {
                    //获取输入到控件的值
                    var UserCode = $("#UserCode").val();
                    //使用异步验证
                    $.post("/Item/CheckUnique", { code: UserCode }, function (data) {
                        if (data == "NO") {
                            alert("用户名已经存在，请您检查");
                            $("#UserCode").val("");
                            return;
                        }
                    });
                }
            });
        }

