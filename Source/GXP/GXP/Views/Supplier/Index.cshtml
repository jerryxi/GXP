                //iconCls: 'icon-save',
                height: gridheight,
                nowrap: true,
                autoRowHeight: false,
                striped: true,
                collapsible: true,
                url: '/Supplier/GetAllSupplier',
                sortName: 'SupplierID',
                sortOrder: 'asc',
                border: true,
                remoteSort: true,
                idField: 'SupplierID',
                loadMsg: '请稍等...',
