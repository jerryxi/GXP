            $("#SupplierID").val("");
            $("#SupplierCode").val("");
            $("#SupplierName").val("");
            $("#LoginPwd").val("");
            $("#Email").val("");
            $("#ContactAddress").val("");
            $("#Contact").val("");
            $("#Region").val("");
            $("#Stage").val("");
            $("#PostCode").val("");
            $("#ContactPhone").val("");
            $("#MobilePhone").val("");
            $("#QQ").val("");
            $("#Msn").val("");
            $("#Fax").val("");
            $("#WebUrl").val("");
            $("#Logo").val("");
            $("#CompanyIntro").val("");
            $("#CompanyCulture").val("");
            $("#ArtificialPerson").val("");
            $("#Remark").val("");
