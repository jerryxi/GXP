            initTable();
            
            //注册查询按钮事件
            SearchSupplierInfo();

            //注册新增弹出框的保存按钮事件
            BindRegistButtonClickEvent();

            //注册新增弹出框的保存并继续按钮事件
            BindRegistAndContinueButtonClickEvent();

